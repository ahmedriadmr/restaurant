<?xml version="1.0" encoding="utf-8"?>
<com.doubleclick.swipetoactionlayout.SwipeToActionLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipe_to_action_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:sal_gravity="right"
    app:sal_isFullActionSupported="false"
    app:sal_items="@menu/swipe_to_action_menu"
    app:sal_shouldVibrateOnQuickAction="true">

    <include
        android:id="@+id/item_cart"
        layout="@layout/item_cart"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</com.doubleclick.swipetoactionlayout.SwipeToActionLayout>